def filepath = testRunner.testCase.getTestStepByName("Properties").getPropertyValue("dataFilePath")

File dataFile = new File(filepath)

def filecontent = dataFile.getText()

def jsonData = new JsonSlurper().parseText(filecontent)

def clog(prop, value) {
	log.info("Property '" + prop + "' changed to '" + value + "'")
}

def jsonDataLength = jsonData.size()
for (int i = 0; i < jsonDataLength; i++){
	testRunner.testCase.getTestStepByName("Properties").setPropertyValue("format", jsonData[i].format)
	clog("format", jsonData[i].format)
	testRunner.testCase.getTestStepByName("Properties").setPropertyValue("lang", jsonData[i].lang)
	clog("lang", jsonData[i].lang)
	testRunner.testCase.getTestStepByName("Properties").setPropertyValue("text", jsonData[i].text)
	clog("text", jsonData[i].text)
	testRunner.testCase.getTestStepByName("Properties").setPropertyValue("expected_result", jsonData[i].expected_result)
	clog("expected_result", jsonData[i].expected_result)

	def runFromTestRunner = testRunner.testCase.getTestStepByName("Properties").getPropertyValue("runFromTestRunner")
	if (runFromTestRunner.equals("true") && i == jsonDataLength - 1) {
